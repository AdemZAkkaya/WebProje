@model IEnumerable<DergiProjesi.Models.Category>

<div class="well">
    <h4>Journal Categories</h4>
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="col-lg-6">
                <ul class="list-unstyled">
                    <li><a href="@Url.Action("Index", "Journal", new { id = item.Id })">@item.Name</a></li>
                </ul>
            </div>
        }
    </div>
</div>